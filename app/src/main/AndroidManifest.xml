<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.runtourist">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.RunTourist">
        <activity android:name=".affichage.MainAccueil" android:theme="@style/Theme.RunTourist.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".affichage.MainActivity"/>
        <activity android:name=".affichage.MainActivity2"/>
        <activity android:name=".affichage.MainFavoris"/>

    </application>

</manifest>